package com.company;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        float a = 10.2f;
        short s = (short) a;
        System.out.println(s);
        long aa = 10;
        double bb = 10.1;
        aa = (long) bb;
        Scanner sc = new Scanner(System.in);
    }
}
